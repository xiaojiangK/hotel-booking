.page{
    width: 100%;
    height: 100%;
    background: url('https://pbwci.qun.hk/FkaETZEXL5ddF4315vxfyRSYV22G') center center no-repeat;
    background-size: cover;
}
.container{
    color: #fff;
    text-align: center;
    padding: 60rpx 50rpx 196rpx;
}
.container image{
    width: 520rpx;
    height: 520rpx;
}
.container .title{
    font-size: 50rpx;
    line-height: 70rpx;
    margin: 0 0 36rpx;
}
.container .desc{
    font-size: 36rpx;
    line-height: 50rpx;
    margin: 0 0 120rpx;
}
.container navigator{
    width: 100%;
    height: 96rpx;
    font-size: 38rpx;
    line-height: 96rpx;
    text-align: center;
    background: #FF6634;
    border-radius: 5px;
}










