/**
 *  个人中心订单详情页
 */
 page{
   height: 100%;
   overflow: hidden;
 }
.order-status{
  padding: 28rpx 0 32rpx 32rpx;
  border-bottom: 2rpx solid #ECECEC;
}
.order-status .title{
  color: #181818;
  font-size: 42rpx;
  line-height: 60rpx;
}
.order-status .time{
  color: #FF6634;
  font-size: 24rpx;
}
.order-status .cancel{
  color: #929292;
}
.sum-price{
  padding: 40rpx 32rpx;
}
.sum-price .item{
  margin: 0 0 6rpx;
}
.sum-price .item .title{
  width: 112rpx;
  font-size: 28rpx;
  color: #181818;
  line-height: 40rpx;
  margin: 0 10rpx 0 0;
  display: inline-block;
}
.sum-price .item .price{
  color: #FF6634;
  font-size: 46rpx;
  line-height: 46rpx;
}
.sum-price .item .price text{
  font-size: 28rpx;
  margin: 0 4rpx 0 0;
}
.sum-price .item .desc{
  color: #919191;
  font-size: 28rpx;
  line-height: 40rpx;
  margin: 0 0 0 24rpx;
}
.sum-price .item .succ{
  color: #FF6634;
}
.detail-list{
  margin: 20rpx 0 0;
  background: #fff;
  overflow: hidden;
}
.detail-list .fold-btn{
  margin: 0 auto 50rpx;
}
.detail-list .fold-btn .btn{
  width: 186rpx;
  height: 40rpx;
  margin: 0 auto;
  color: #B2B2B2;
  font-size: 24rpx;
  background: #fff;
  line-height: 40rpx;
  text-align: center;
  position: relative;
  border-radius: 40rpx;
  border: 2rpx solid #ECECEC;
}
.detail-list .fold-btn{
  width: 100%;
  height: 20rpx;
  border-bottom: 2rpx dashed #ECECEC;
}
/*** 费用信息 ***/
.cost {
  margin-top: 20rpx;
  background: #fff;
}
.cost .row {
  display: flex;
  justify-content: space-between;
  padding: 0 54rpx 0 21rpx;
  color: #181818;
  font-size: 30rpx;
  font-weight: 400;
  line-height: 80rpx;
}
.cost .row:first-of-type {
  background: #fff7e9;
}
.cost .row view:nth-of-type(2) {
  font-weight: bold;
}
.cost .row .del {
  margin-left: 34rpx;
  color: #8e8e8e;
  font-size: 24rpx;
  font-weight: 400;
  text-decoration: line-through;
}
.cost .row .tips {
  color: #ff3b36;
  line-height: 1;
  text-align: right;
}
.cost .row .tips view:first-of-type {
  padding-top: 13rpx;
  font-size: 30rpx;
  font-weight: bold;
}
.cost .row .tips view:last-of-type {
  padding-top: 11rpx;
  font-size: 22rpx;
  font-weight: 400;
}
.cost .row .date {
  margin-left: 20rpx;
  font-size: 26rpx;
}
.cost .row .breakfast {
  margin-left: 73rpx;
  color: #929292;
}
.cost .row view:nth-of-type(2) text {
  font-weight: bold;
}
.cost .icon-info {
  margin-left: 20rpx;
  margin-bottom: -3rpx;
}
.cost .row .deduction {
  color: #ff3b36;
}
.remind {
  padding: 34rpx 34rpx 37rpx;
  color: #929292;
  font-size: 26rpx;
  line-height: 30rpx;
  text-align: center;
  background: #f2f2f2;
}
.detail-list .list{
  display: none;
  transition: .3s all;
  border-bottom: 2rpx solid #ECECEC;
}
.detail-list .show{
  display: block;
}
.detail-list .item{
  height: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
  padding: 0 54rpx 0 20rpx;
}
.detail-list .first{
  background: #FFF6F0;
}
.detail-list .item .title{
  color: #181818;
  font-size: 30rpx;
}
.detail-list .left{
  float: left;
  font-size: 30rpx;
}
.detail-list .left image{
  width: 24rpx;
  height: 24rpx;
  margin: 0 0 0 18rpx;
  vertical-align: middle;
}
.detail-list .right{
  float: right;
  color: #181818;
  font-size: 30rpx;
}
.detail-list .gold .right{
  color: #FF6634;
}
.detail-list .time{
  padding: 0 54rpx 0 40rpx;
}
.detail-list .time .time-item{
  width: 100%;
  height: 40rpx;
  font-size: 26rpx;
  padding: 0 0 22rpx;
  overflow: hidden;
  color: #181818;
}
.detail-list .time .time-item .date{
  float: left;
}
.detail-list .time .time-item text{
  color: #929292;
  float: left;
  margin: 0 0 0 72rpx;
}
.detail-list .time .time-item .price{
  float: right;
  font-size: 30rpx;
}
.control{
  margin: 40rpx 0 0;
  padding: 0 0 40rpx;
  border-bottom: 20rpx solid #F0F0F0;
}
.control .btns{
  display: flex;
  justify-content: center;
  margin: 0 32rpx 30rpx;
}
.control .btns .btn {
  border: 2rpx solid transparent;
  border-radius: 4rpx;
  width: 326rpx;
  height: 76rpx;
  font-size: 30rpx;
  line-height: 76rpx;
  text-align: center;
}
.control .btns .btn.white {
  border-color: #ff6342;
  color: #ff6342;
  background: #fff;
  margin: 0 30rpx 0 0;
}
.control .btns .btn.reply{
  margin: 0 30rpx 0 0;
}
.control .btns .btn.orange {
  border-color: #ff6342;
  color: #fff;
  background: #ff6342;
}
.control .btns .btn.gray {
  border-color: #ccc;
  background: #ccc;
  margin: 0 30rpx 0 0;
  color: #fff;
}
.control .tip{
  margin: 0 32rpx;
  color: #929292;
  font-size: 26rpx;
  line-height: 36rpx;
}
.info{
  padding: 36rpx 30rpx 0;
  border-bottom: 20rpx solid #F0F0F0;
}
.info>.title{
  color: #181818;
  font-size: 28rpx;
  line-height: 40rpx;
  margin: 0 0 26rpx;
  background: url('https://pbwci.qun.hk/FrPW-iJgKElkFPsWDhcI4xMjacv2') right center no-repeat;
  background-size: 11rpx 21rpx;
}
.info .content{
  padding: 0 0 20rpx;
  margin: 0 0 34rpx;
  border-bottom: 2rpx solid #ECECEC;
}
.info .content .title{
  color: #181818;
  font-size: 32rpx;
  margin: 0 0 20rpx;
  line-height: 46rpx;
}
.info .content .desc{
  color: #333;
  font-size: 26rpx;
  margin: 0 0 28rpx;
  line-height: 37rpx;
  padding: 0 140rpx 0 0;
}
.info:last-child{
  border-bottom: none;
}
.info .menu{
  display: flex;
}
.info .menu .item{
  flex: 1;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-right: 2rpx solid #ECECEC;
}
.info .menu .item:last-child{
  border: none;
}
.info .menu .item image{
  width: 28rpx;
  height: 28rpx;
  margin: 0 20rpx 0 0;
}
.info .menu .item text{
  color: #595959;
  font-size: 28rpx;
  line-height: 40rpx;
}
.info .list .item{
  overflow: hidden;
  padding: 0 0 18rpx;
  line-height: 40rpx;
}
.info .list .item .title{
  float: left;
  color: #929292;
  font-size: 28rpx;
}
.info .list .item .desc{
  float: right;
  color: #181818;
  font-size: 28rpx;
}
.info .list .item .desc view{
  font-size: 24rpx;
  color: #929292;
  text-align: right;
  line-height: 34rpx;
}