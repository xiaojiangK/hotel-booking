.wrap{
    background: #fff;
}
.search{
    padding: 26rpx 30rpx 0;
    overflow: hidden;
}
.search .item{
    float: left;
    height: 72rpx;
    position: relative;
    line-height: 72rpx;
    background: #F4F5F9;
}
.search .city{
    width: 145rpx;
}
.search .date{
    width: 216rpx;
}
.search .date text{
    display: block;
    line-height: 30rpx;
}
.search .date .today{
    float: left;
    width: 68rpx;
    height: 32rpx;
    color: #fff;
    font-size: 24rpx;
    margin: 22rpx 0 0;
    background: #333;
    line-height: 32rpx;
    text-align: center;
    border-radius: 4rpx;
}
.search .form{
    width: 330rpx;
}
.search .form text{
    float: left;
    width: 226rpx;
    color: #999;
    font-size: 24rpx;
    overflow: hidden;
    text-indent: 10rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.search .form .active{
    color: #333;
}
.search .form .close-icon{
    width: 36rpx;
    height: 36rpx;
    float: right;
    margin: 18rpx 18rpx 0 0;
}
.search .item .text{
    font-size: 24rpx;
    float: left;
    margin: 0 0 0 30rpx;
}
.search .city .text{
    width: 95rpx;
    color: #666;
    overflow: hidden;
    margin: 0 0 0 20rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.search .date .text{
    color: #888;
    margin: 8rpx 18rpx 0;
}
.down-icon, .up-icon{
    width: 12rpx;
    height: 8rpx;
    float: right;
    margin: 32rpx 24rpx 0 0;
}
.search .city image{
    margin: 32rpx 18rpx 0 0;   
}
.search .item::after{
    content: '';
    width: 2rpx;
    height: 26rpx;
    display: block;
    background: #DCD8E0;
    position: absolute;
    right: 0;
    top: 26rpx;
}
.search  .form::after{
    display: none;
}
.search .search-icon{
    width: 28rpx;
    height: 28rpx;
    float: left;
    margin: 22rpx 6rpx 0 16rpx;
}
.select{
    height: 70rpx;
    position: relative;
    box-shadow: 0 6rpx 15rpx 0 rgba(10,31,68,0.12);
}
.select>.item{
    width: 25%;
    float: left;
    height: 70rpx;
    text-align: center;
    line-height: 70rpx;
}
.select>.item>text{
    color: #181818;
    font-size: 26rpx;
    padding: 0 10rpx 0 20rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: table-cell;
    vertical-align: middle;
}
.select>.item .icon{
    height: 70rpx;
    display: table-cell;
    vertical-align: middle;
}
.select>.item .icon image{
    margin: 0;
}
.select>.active>text{
    color: #FF6342;
}
.select>.active .down{
    transform: rotate(180deg);
}
.select .menu .main{
    background: #fff;
    position: absolute;
    box-sizing: border-box;
    z-index: 9;
    width: 100%;
    top: 70rpx;
    left: 0;
    z-index: 100;
}
.select .menu .mask{
    position: fixed;
    top: 320rpx;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 99;
    background: rgba(0,0,0,.5);
}
.select .menu scroll-view{
    height: 720rpx;
}
.select .menu .left{
    width: 200rpx;
    float: left;
    height: 100%;
    background: #F4F5F8;
}
.select .menu .left .item{
    width: 100%;
    height: 90rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.select .menu .left .active{
    background: #fff;
}
.select .menu .left text{
    color: #333;
    font-size: 28rpx;
    line-height: 90rpx;
    display: block;
}
.select .menu .left image{
    width: 22rpx;
    height: 16rpx;
    margin: 0 20rpx 0 0;
}
.select .menu1 .right .item{
    overflow: hidden;
    height: 88rpx;
    line-height: 88rpx;
    border-bottom: 2rpx solid #E5E5E5;
}
.select .menu .right text{
    float: left;
    color: #606060;
    font-size: 28rpx;
}
.select .menu .right image{
    width: 22rpx;
    height: 16rpx;
    float: right;
    margin: 34rpx 32rpx 0 0;
}
.select .menu .right{
    width: 550rpx;
    float: right;
    background: #fff;
    box-sizing: border-box;
    padding: 0 0 148rpx 40rpx;
}
.select .menu .active text{
    color: #FF6342;
}
.select .menu2 .title{
    color: #181818;
    text-align: left;
    margin: 22rpx 32rpx 13rpx;
    line-height: 46rpx;
}
.select .menu2 .menu-list{
    margin: 0 32rpx;
    overflow: hidden;
}
.select .menu2 .item{
    width: 164rpx;
    height: 70rpx;
    float: left;
    color: #5A5A5A;
    font-size: 24rpx;
    line-height: 70rpx;
    text-align: center;
    border-radius: 6rpx;
    background: #F4F5F9;
    margin: 0 10rpx 12rpx 0;
}
.select .menu2 .item:nth-of-type(4n){
    margin: 0 0 12rpx 0;
}
.select .menu2 .active, .select .menu4 .active{
    color: #FF6342;
    background: #FFF1F2;
}
.select .menu2 .btns{
    overflow: hidden;
    margin: 56rpx 0 0;
}
.select .menu2 .btns .reset, .select .menu4 .btns .reset{
    float: left;
    width: 250rpx;
    height: 96rpx;
    color: #5A5A5A;
    font-size: 32rpx;
    text-align: center;
    line-height: 96rpx;
    border-top: 2rpx solid #E5E5E5;
}
.select .menu2 .btns .submit, .select .menu4 .btns .submit{
    float: left;
    width: 500rpx;
    height: 96rpx;
    color: #fff;
    font-size: 32rpx;
    line-height: 96rpx;
    text-align: center;
    background: #FF6342;
}
.select .menu3 .item{
    height: 90rpx;
    padding: 0 25rpx 0 50rpx;
    overflow: hidden;
    line-height: 90rpx;
    border-bottom: 2rpx solid #E5E5E5;
}
.select .menu3 .item text{
    float: left;
    color: #606060;
    font-size: 28rpx;
}
.select .menu3 .active text{
    color: #FF6342;
}
.select .menu3 .item image{
    width: 22rpx;
    height: 16rpx;
    float: right;
    margin: 38rpx 0 0 0;
}
.select .menu4 .main, .select .menu1 .main{
    background: #F4F5F8;
}
.select .menu4 .right{
    background: #fff;
}
.select .menu4 .right .item{
    width: 144rpx;
    height: 70rpx;
    font-size: 24rpx;
    color: #5A5A5A;
    line-height: 70rpx;
    text-align: center;
    border-radius: 6rpx;
    background: #F4F5F9;
    margin: 18rpx 16rpx 0  0;
    border: none;
    float: left;
}
.select .menu4 .right .item:nth-of-type(3n){
    margin: 18rpx 0 0;
}
.select .menu4 .right .active{
    color: #FF6342;
    background: #FFF1F2;
}
.select .menu4 .btns{
    width: 100%;
    overflow: hidden;
}
.content{
    overflow: hidden;
}
.content .backTop{
    width: 104rpx;
    height: 104rpx;
    position: fixed;
    right: 40rpx;
    bottom: 40rpx;
    z-index: 999;
    border-radius: 50%;
}
.content .backTop image{
    width: 100%;
    height: 104rpx;
}
.content .list .loading{
    text-align: center;
    margin: 30rpx 0;
}
.content .loading text{
    color: #999;
    font-size: 28rpx;
    line-height: 40rpx;
}
.content .loading-icon{
    width: 36rpx;
    height: 36rpx;
    margin: 0 28rpx 0 0;
    vertical-align: middle;
    animation:retate 1s infinite;
}
@keyframes retate {
    from {
        transform: rotate(0);
    }
    to {
        transform: rotate(360deg);
    }
}
.list{
    overflow: hidden;
    padding: 24rpx 0 0 24rpx;
}
.list .item{
    position: relative;
    overflow: hidden;
    margin: 0 0 18rpx;
}
.list .item .picture{
    width: 192rpx;
    display: table-cell;
    vertical-align: middle;
}
.list .item .picture image{
    width: 100%;
    height: 220rpx;
    background: #D1D1D1;
}
.list .item .desc{
    width: 510rpx;
    display: table-cell;
    padding: 0 0 0 20rpx;
    vertical-align: top;
    position: relative;
    border-bottom: 2rpx solid #E0E0E0;
}
.list .item .desc .title{
    width: 475rpx;
    height: 46rpx;
    color: #181818;
    font-size: 32rpx;
    overflow: hidden;
    line-height: 46rpx;
    margin: 0 0 12rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.list .item .desc .detail{
    overflow: hidden;
}
.list .item .desc .info{
    float: left;
    color: #666;
    font-size: 24rpx;
    line-height: 34rpx;
}
.list .item .desc .address{
    overflow: hidden;
    max-height: 66rpx;
    line-height: 32rpx;
    margin: 10rpx 0;
}
.list .item .info .star{
    color: #888;
}
.list .item .desc .star .num{
    color: #008CF3;
    font-size: 36rpx;
}
.list .item .desc .star .text{
    color: #008CF3;
    font-size: 22rpx;
    margin: 0 18rpx 0 4rpx;
}
.list .item .desc .price{
    position: absolute;
    right: 24rpx;
    bottom: 6rpx;
}
.list .item .desc .old-price{
    color: #B8B8B8;
    font-size: 24rpx;
    text-decoration: line-through;
}
.list .item .desc .price-text{
    color: #888;
    font-size: 24rpx;
    margin: 4rpx 0 0;
    text-align: right;
}
.list .item .price-text text{
    color: #FF6342;
    font-size: 40rpx;
}
.list .item .price-text text:last-child{
    margin: 0 12rpx 0 0;
}
.list .item .price-text .icon{
    font-size: 24rpx;
    margin: 0 2rpx 0 0;
}
.list .item .desc .member{
    width: 268rpx;
    position: relative;
}
.list .item .desc .member text{
    position: absolute;
    color: #6D4727;
    left: 130rpx;
    z-index: 99;
    top: 0;
}